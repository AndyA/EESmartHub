{
  "name": "eesmarthub",
  "version": "0.0.1",
  "description": "Monitor an EE Smart Hub",
  "main": "index.js",
  "bin": {
    "eesmarthub": "poller.js"
  },
  "scripts": {
    "dev": "nodemon bin/app.js"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "consistent",
    "jsxSingleQuote": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf"
  },
  "dependencies": {
    "app-module-path": "^2.2.0",
    "bluebird": "^3.7.2",
    "config": "^3.3.8",
    "cross-fetch": "^3.0.6",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "jsonpath-faster": "^0.0.12",
    "jsonpath-lifter": "^0.0.13",
    "lodash": "^4.17.21",
    "prom-client": "^14.0.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndyA/EESmartHub.git"
  },
  "keywords": [
    "router",
    "ee",
    "smarthub"
  ],
  "author": "Andy Armstrong <andy@hexten.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AndyA/EESmartHub/issues"
